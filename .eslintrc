{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "arrow-parens": "off",
        "indent": "off",
        "require-jsdoc": "off",
        "comma-dangle": "off",
        "object-curly-spacing": "off",
        "operator-linebreak": "off",
        "no-debugger": "off",
        "space-before-function-paren": "off"
        
    },
    "extends": ["eslint:recommended", "google"]
}